{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "certManager": {
            "type": "object",
            "properties": {
                "namespace": {
                    "type": "string"
                },
                "serviceAccountName": {
                    "type": "string"
                }
            }
        },
        "clusterIssuer": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                },
                "production": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        }
                    }
                },
                "staging": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "duckdns": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string"
                }
            }
        },
        "secret": {
            "type": "object",
            "properties": {
                "existingSecret": {
                    "type": "boolean"
                },
                "existingSecretName": {
                    "type": "string"
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "groupName": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "logLevel": {
            "type": "integer"
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "resources": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
